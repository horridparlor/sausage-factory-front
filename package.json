{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "test": "vitest run",
    "preview": "vite preview",
    "test:playwright": "npx playwright test",
    "test:playwright:report": "npx playwright show-report",
    "type:check": "tsc --noEmit",
    "lint:check": "eslint .",
    "lint:write": "eslint --fix .",
    "format:check": "prettier --check './src/**/*.{ts,tsx}'",
    "format:write": "prettier --write './src/**/*.{ts,tsx}'",
    "api:generate": "npx tsp compile main.tsp --emit @typespec/openapi3 --output-dir ./.api && openapi-typescript ./.api/@typespec/openapi3/openapi.yaml -o ./src/api/schema.d.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "6.0.2",
    "@mui/material": "^6.1.4",
    "@mui/system": "^6.1.4",
    "@mui/utils": "^6.1.4",
    "@mui/x-data-grid": "^7.20.0",
    "dom-to-image": "2.6.0",
    "i18next": "^23.15.1",
    "js-cookie": "3.0.5",
    "jsdom": "^25.0.1",
    "lodash": "4.17.21",
    "openapi-fetch": "^0.12.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^15.0.2",
    "react-router": "6.26.1",
    "react-router-dom": "6.26.1",
    "react-toastify": "10.0.5"
  },
  "devDependencies": {
    "@eslint/js": "9.9.0",
    "@playwright/test": "^1.47.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/dom-to-image": "2.6.7",
    "@types/js-cookie": "3.0.6",
    "@types/lodash": "4.17.7",
    "@types/node": "^22.5.5",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typespec/compiler": "latest",
    "@typespec/http": "latest",
    "@typespec/openapi3": "latest",
    "@typespec/rest": "latest",
    "@vitejs/plugin-react-swc": "3.5.0",
    "eslint": "9.9.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "5.1.0-rc.0",
    "eslint-plugin-react-refresh": "0.4.9",
    "globals": "15.9.0",
    "husky": "^9.1.6",
    "openapi-typescript": "^7.4.1",
    "prettier": "3.3.3",
    "typescript": "5.5.3",
    "typescript-eslint": "8.0.1",
    "vite": "5.4.1",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^2.1.2"
  }
}
